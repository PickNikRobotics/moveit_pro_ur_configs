<?xml version="1.0" encoding="utf-8" ?>
<root BTCPP_format="4" main_tree_to_execute="Cycle Between Poses">
  <!--//////////-->
  <BehaviorTree
    ID="Cycle Between Poses"
    _description="Simple example objective for multi arm motions. Two arms are moved to different waypoints."
    _favorite="true"
    _hardcoded="false"
  >
    <Decorator ID="KeepRunningUntilFailure">
      <Control ID="Sequence">
        <SubTree
          ID="Move to Waypoint"
          _collapsed="true"
          joint_group_name="multi_arm_manipulator"
          controller_names="/multi_arm_joint_trajectory_controller"
          velocity_scale_factor="1.0"
          acceleration_scale_factor="1.0"
          link_padding="0.0"
          keep_orientation_tolerance="0.05"
          keep_orientation="false"
          waypoint_name="Home"
          target_joint_state="{target_joint_state}"
          controller_action_server="/multi_arm_joint_trajectory_controller/follow_joint_trajectory"
        />
        <SubTree
          ID="Move to Waypoint"
          _collapsed="true"
          joint_group_name="multi_arm_manipulator"
          controller_names="/multi_arm_joint_trajectory_controller"
          velocity_scale_factor="1.0"
          acceleration_scale_factor="1.0"
          link_padding="0.0"
          keep_orientation_tolerance="0.05"
          keep_orientation="false"
          waypoint_name="Flip"
          target_joint_state="{target_joint_state}"
          controller_action_server="/multi_arm_joint_trajectory_controller/follow_joint_trajectory"
        />
      </Control>
    </Decorator>
  </BehaviorTree>
  <TreeNodesModel>
    <SubTree ID="Cycle Between Poses" />
  </TreeNodesModel>
</root>
